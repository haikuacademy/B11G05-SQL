-- CREATE

INSERT INTO houses (
    house_id,
    house_location,
    house_rooms,
    house_bathrooms,
    description,
    user_id,
    price,
    reviews_count,
    ratings
)
VALUES (
    29,
    'Phuket, Thailand',
    3,
    1,
    'Lovely bungalow on the beach. Escape to Paradise!',
    3,
    24,
    18,
    4.5)
    ;
cgdcgd
-- READ

SELECT photo_link, house_location, house_rooms, house_bathrooms, price, date_start, date_end, message, total_nights, price_total, reviews_count, reviews.rating, reviews.comment
FROM houses
LEFT JOIN house_photos
ON
houses.house_id = house_photos.house_id
LEFT JOIN reviews
ON
houses.house_id = reviews.house_id
LEFT JOIN bookings
ON
houses.house_id = bookings.house_id
;

-- UPDATE

UPDATE houses
SET
house_location = 'Bali, Indonesia',
house_rooms = 2,
house_bathrooms = 2,
price = 30,
description = 'Forget your troubles in paradise! Close to many activities including hiking, ATVs, restaurants, and the beach, this lovely villa is the perfect island getaway.'
WHERE house_id = 34
;

